# Server
PORT=3000
NODE_ENV=development
CORS_ORIGINS=http://localhost:3000

# Database
DATABASE_TYPE=sqlite
DATABASE_PATH=database.sqlite

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_TLS=false

# ChromaDB
CHROMADB_HOST=localhost
CHROMADB_PORT=8000
CHROMADB_API_KEY=
CHROMADB_TLS=false

# LLM Providers
CLAUDE_API_KEY=
OPENAI_API_KEY=
OPENROUTER_API_KEY=

# Ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_TLS=false

# Agent LLM Configurations
AGENT_PROJECT_MANAGER_PROVIDER=claude
AGENT_PROJECT_MANAGER_MODEL=claude-3-opus-20240229
AGENT_ARCHITECT_PROVIDER=claude
AGENT_ARCHITECT_MODEL=claude-3-opus-20240229
AGENT_FRONTEND_DEVELOPER_PROVIDER=claude
AGENT_FRONTEND_DEVELOPER_MODEL=claude-3-opus-20240229
AGENT_BACKEND_DEVELOPER_PROVIDER=claude
AGENT_BACKEND_DEVELOPER_MODEL=claude-3-opus-20240229
AGENT_CODE_REVIEWER_PROVIDER=claude
AGENT_CODE_REVIEWER_MODEL=claude-3-opus-20240229
AGENT_DEVOPS_PROVIDER=claude
AGENT_DEVOPS_MODEL=claude-3-opus-20240229
AGENT_QA_ENGINEER_PROVIDER=claude
AGENT_QA_ENGINEER_MODEL=claude-3-opus-20240229

# Default provider settings
DEFAULT_PROVIDER=claude
DEFAULT_MODEL=claude-3-opus-20240229

# Agent settings
AGENT_MEMORY_TTL=3600
MAX_CONCURRENT_TASKS=5

# Security
API_KEY_SALT=your-api-key-salt
JWT_SECRET=your-jwt-secret

# Logging
LOG_LEVEL=info
LOG_FORMAT=json